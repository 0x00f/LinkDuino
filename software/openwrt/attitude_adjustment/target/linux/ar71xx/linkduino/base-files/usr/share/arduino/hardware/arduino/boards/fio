
config upload 'upload'
	option protocol 'arduino'
	option maximum_size '30720'
	option speed '57600'

config bootloader 'bootloader'
	option low_fuses '0xFF'
	option high_fuses '0xDA'
	option extended_fuses '0x05'
	option path 'arduino:atmega'
	option file 'ATmegaBOOT_168_atmega328_pro_8MHz.hex'
	option unlock_bits '0x3F'
	option lock_bits '0x0F'

config build 'build'
	option mcu 'atmega328p'
	option f_cpu '8000000L'
	option core 'arduino'
	option variant 'eightanaloginputs'
