
config upload 'upload'
	option maximum_size '126976'
	option speed '57600'

config bootloader 'bootloader'
	option high_fuses '0xDA'
	option extended_fuses '0xF5'
	option path 'atmega'
	option file 'ATmegaBOOT_168_atmega1280.hex'
	option unlock_bits '0x3F'
	option lock_bits '0x0F'

config build 'build'
	option f_cpu '16000000L'
	option core 'arduino'
	option variant 'mega'
